// Learn more about this file in the docs :https://www.prisma.io/docs/concepts/components/prisma-schema

datasource db {
   provider = "postgresql"
   url = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

model Todo {
  uid String @id @default(cuid())
  create_at DateTime
  text String
  done Boolean
}